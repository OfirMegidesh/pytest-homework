def add(a, b):
    z = a + b
    return z


def subtract(a, b):
    z = a - b
    return z


def multiply(a, b):
    z = a * b
    return z


def divide(a, b):
    z = a / b
    return z


def divide_evenlly(a, b):
    z = a // b
    return z


def mudolo(a, b):
    z = a % b
    return z


def sqrt(a):
    z = a ** 0.5
    return z
